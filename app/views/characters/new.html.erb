<h1>Create your Adventurer</h1>

<%= form_for (@character) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %><br/>

<br/>
<br/>

<%= f.label :race %>
<%= f.text_field :race %><br/>

<br/>
<br/>

<%= f.label :klass, "Class" %>
<%= f.text_field :klass %><br/>

<br/>
<br/>

<%= f.submit %>
<% end %>